{
  "name": "tree-sitter-quanty",
  "version": "0.20.2",
  "description": "Quanty grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parser",
    "lexer",
    "quanty"
  ],
  "author": "Flo Leroux",
  "dependencies": {
    "nan": "^2.18.0"
  },
  "repository": {
      "type": "git",
      "url": "https://github.com/tree-sitter/tree-sitter-quanty.git"
  },
  "devDependencies": {
    "eslint": ">=8.56.0",
    "eslint-config-google": "^0.14.0",
    "tree-sitter-cli": "^0.20.8",
    "node-gyp": "^10.0.1"
  },
  "scripts": {
    "build": "tree-sitter generate && node-gyp build",
    "lint": "eslint grammar.js",
    "test": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.qy",
      "file-types": [
        "qy",
        "quanty"
      ],
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ]
}