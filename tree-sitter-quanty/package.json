{
  "name": "tree-sitter-quanty",
  "version": "0.20.2",
  "description": "Quanty grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parser",
    "lexer",
    "quanty"
  ],
  "author": "Flo Leroux",
  "dependencies": {
    "nan": "^2.18.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Flo-Leroux/quanty-lang/tree/feature/tree-sitter/tree-sitter-quanty"
  },
  "devDependencies": {
    "eslint": ">=8.56.0",
    "eslint-config-google": "^0.14.0",
    "node-gyp": "^10.0.1",
    "nodemon": "^3.0.3",
    "tree-sitter-cli": "^0.20.8"
  },
  "scripts": {
    "build": "pnpm generate && node-gyp build",
    "lint": "eslint grammar.js",
    "test": "tree-sitter test",
    "generate": "tree-sitter generate",
    "parse": "tree-sitter parse",
    "dev": "nodemon -w ./examples -w ./grammar.js -e qy,js,quanty --exec tree-sitter",
    "wasm": "tree-sitter build-wasm",
    "playground": "pnpm generate && pnpm wasm && tree-sitter playground"
  },
  "tree-sitter": [
    {
      "scope": "source.qy",
      "file-types": [
        "qy",
        "quanty"
      ],
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ]
}